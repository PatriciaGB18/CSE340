
<h1><%= title %></h1>
<%- messages() %>
<p>You're logged in</p>
<h2>
    Welcome <%= locals.accountData.account_firstname %>
</h2>

<p><a href="/account/update/<%= locals.accountData.account_id %>" title="Click to update account information">
    Update Account Information
</a></p>

<% if (locals.accountData.account_type === "Employee" || locals.accountData.account_type === "Admin") { %>
    <h3>Inventory Management</h3>
    <p><a href="/inv/" title="Click to manage inventory">
        Access Inventory Management
    </a></p>
<% } %>